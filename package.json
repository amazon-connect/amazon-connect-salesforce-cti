{
  "name": "amazon-connect-salesforce-cti",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build-pdf": "docusaurus build && docusaurus-pdf from-build build/ docs/lightning/notices /amazon-connect-salesforce-cti/ --output-file pdf/lightning.pdf && docusaurus-pdf from-build build/ docs/classic/notices /amazon-connect-salesforce-cti/ --output-file pdf/classic.pdf",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "init-githooks": "git config core.hooksPath .githooks"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "papersaurus": "1.4.0",
    "@bojl/docusaurus-pdf": "1.2.0",
    "is-svg": ">=4.2.2",
    "react-dev-utils": ">=11.0.4",
    "immer": ">=8.0.1",
    "highlight.js": ">=10.4.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
